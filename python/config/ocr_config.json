    "preprocessing": {
        "denoise": true,
        "adaptive_threshold": true,
        "contrast_enhancement": false,
        "deskew": false
    },
    "output": {
        "save_visualizations": true,
        "save_json_results": true,
        "image_format": "png"
    }
}