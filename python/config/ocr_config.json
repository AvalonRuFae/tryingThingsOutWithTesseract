{
    "tesseract": {
        "default_config": "--oem 3 --psm 6",
        "language": "eng",
        "char_whitelist": "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789.,!?'\"-()[]{}@ ",
        "confidence_threshold": 60
